{
	"image": "docker.io/library/golang:1.25",
	"name": "adventofcode",
	"workspaceMount": "",
	"runArgs": [
		"--userns=keep-id",
		"--volume=${localWorkspaceFolder}:/workspaces/${localWorkspaceFolderBasename}:Z"
